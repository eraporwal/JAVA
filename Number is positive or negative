class Test {
    public static void main(String[] args) {
      int n=-6;
      if(n>0){
        System.out.println("Number is positive");
      }
      else if(n==0){
        System.out.println("Number is Neutral");
      }
      else
      {
        System.out.println("Number is Negative");
      }
      }
       
      }
   
