public class Practice {

    public static void main(String[] args) {

        int n = 30;
        if (n % 2 == 0 && n > 1 && n < 6) {
            System.out.println("Not Weird");
        } else if (n % 2 == 0 && n > 5 && n < 21) {
            System.out.println("Weird");

        } else if (n % 2 == 0 && n > 20) {
            System.out.println("Not Weird");
        } else {
            System.out.println("Weird");
        }

    }
}
