public class SumAndAverage {
    public static void main(String[] args) {
      int age=20;
      if (age>18){
        System.out.println("Eligible for voting");
      }
      else if(age==18){
        System.out.println("might vote");
      }
      else{
        System.out.println("Sorry!you are not eligible for voting");
      }
    }
  }
