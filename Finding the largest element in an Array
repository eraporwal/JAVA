class MaxElement {
 public static void main(String[] args) {
        
        int a[]={2,4,6,7,9,10,12};
        int max=a[0];
       for(int i=0;i<a.length;i++){
            if(a[i]>a[0]){
                max=a[i];
            }
            
        }
        System.out.println(max);
            
        }
        
    }
   

